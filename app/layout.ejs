          </li>
        </ul>
        <span class="material-icons text-primary d-md-none handle">
          drag_handle
        </span>
      </div>
      <div class="d-flex">
        <div class="w-50 border-bottom border-2 border-primary"></div>
        <div class="w-50 border-bottom border-2 border-blue"></div>
      </div>
    </nav>

    <%- contents %>

    <footer class="footer text-center py-16 py-xs-24 bg-sec-bg">
      <p class="text-secondary">Coding Home © 2022 All Rights Reserved</p>
    </footer>
    <script src="./assets/js/vendors.js"></script>
